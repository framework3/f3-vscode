{
  "scopeName": "source.f3",
  "fileTypes": [
    "f3"
  ],
  "name": "F3 Language",
  "patterns": [
    {
      "name": "comment.line.number-sign.f3",
      "match": "#.*$"
    },
    {
      "begin": "(<)(backend)(>)",
      "beginCaptures": {
        "1": { "name": "punctuation.definition.tag.begin.f3" },
        "2": { "name": "entity.name.tag.f3" },
        "3": { "name": "punctuation.definition.tag.end.f3" }
      },
      "end": "(<)(/)(backend)(>)",
      "endCaptures": {
        "1": { "name": "punctuation.definition.tag.begin.f3" },
        "2": { "name": "punctuation.definition.tag.end.f3" },
        "3": { "name": "entity.name.tag.f3" },
        "4": { "name": "punctuation.definition.tag.end.f3" }
      },
      "name": "meta.embedded.block.f3",
      "patterns": [
        { "include": "source.php" }
      ]
    },
    {
      "begin": "(<)(script)(>)",
      "beginCaptures": {
        "1": { "name": "punctuation.definition.tag.begin.f3" },
        "2": { "name": "entity.name.tag.f3" },
        "3": { "name": "punctuation.definition.tag.end.f3" }
      },
      "end": "(<)(/)(script)(>)",
      "endCaptures": {
        "1": { "name": "punctuation.definition.tag.begin.f3" },
        "2": { "name": "punctuation.definition.tag.end.f3" },
        "3": { "name": "entity.name.tag.f3" },
        "4": { "name": "punctuation.definition.tag.end.f3" }
      },
      "name": "meta.embedded.block.f3",
      "patterns": [
        { "include": "source.js" }
      ]
    },
    {
      "begin": "(<)(html)(>)",
      "beginCaptures": {
        "1": { "name": "punctuation.definition.tag.begin.f3" },
        "2": { "name": "entity.name.tag.f3" },
        "3": { "name": "punctuation.definition.tag.end.f3" }
      },
      "end": "(<)(/)(html)(>)",
      "endCaptures": {
        "1": { "name": "punctuation.definition.tag.begin.f3" },
        "2": { "name": "punctuation.definition.tag.end.f3" },
        "3": { "name": "entity.name.tag.f3" },
        "4": { "name": "punctuation.definition.tag.end.f3" }
      },
      "name": "meta.embedded.block.f3",
      "patterns": [
          { "include": "text.html.derivative" },
          {
            "name": "meta.interpolation.f3",
            "match": "\\{\\{[^}]*\\}\\}"
          }
      ]
    }
  ]
}
